{
    "UniLang": {
        "variable": "var ${name} : ${type} = ${value};\n",
        "constant": "const ${name} : ${type} = ${value};\n",
        "reassign": "${name} ${operator}= ${value};\n",
        "import": "import ${library};\n"
    },

    "Java": {
        "variable": "${type} ${name} = ${value};\n",
        "constant": "final ${type} ${name} = ${value};\n",
        "reassign": "${name} ${operator}= ${value};\n",
        "import": "import ${library};\n",
        "types": {
            "Int": "int",
            "Bool": "boolean"

        }
    }, 

    "Swift": {
        "variable": "var ${name} : ${type} = ${value};\n",
        "constant": "let ${name} : ${type} = ${value};\n",
        "reassign": "${name} ${operator}= ${value};\n",
        "import": "import ${library};\n",
        "types": {
            
        }
    }
}